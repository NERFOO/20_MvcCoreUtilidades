@model List<Coche>

    <h1>Vista parcial</h1>

    <table class="table table-striped">
        <thead>
            <tr>
                <th>Id</th>
                <th>Marca</th>
                <th>Modelo</th>
                <th>Imagen</th>
            </tr>
        </thead>
        <tbody>
        @foreach(Coche c in Model)
        {
            <tr>
                <td>@c.IdCoche</td>
                <td>@c.Marca</td>
                <td>@c.Modelo</td>
                <td>
                    <img src="@c.Imagen" style="width: 200px">
                </td>
            </tr>
        }
        </tbody>
    </table>