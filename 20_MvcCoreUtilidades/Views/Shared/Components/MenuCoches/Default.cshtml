
@model List<Coche>

<div class="dropdown">
    <button class="btn btn-secondary dropdown-toggle" type="button" data-bs-toggle="dropdown" aria-expanded="false">
        Menú Coches
    </button>

    <ul class="dropdown-menu">
        @foreach(Coche c in Model)
        {
            <li>
                <a class="dropdown-item" asp-controller="Coches" asp-action="Details" asp-route-idCoche="@c.IdCoche">@c.Marca - @c.Modelo</a>
                <img src="@c.Imagen" style="width: 200px">
            </li>
        }
    </ul>
</div>

